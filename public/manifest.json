{
  "name": "EU Google Maps Links",
  "description": "This extension re-enables links to Google Maps in EU regions",
  "version": "1.2",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "https://*.google.at/search*",
        "https://*.google.be/search*",
        "https://*.google.bg/search*",
        "https://*.google.hr/search*",
        "https://*.google.cy/search*",
        "https://*.google.cz/search*",
        "https://*.google.dk/search*",
        "https://*.google.ee/search*",
        "https://*.google.fi/search*",
        "https://*.google.fr/search*",
        "https://*.google.de/search*",
        "https://*.google.gr/search*",
        "https://*.google.hu/search*",
        "https://*.google.ie/search*",
        "https://*.google.it/search*",
        "https://*.google.lv/search*",
        "https://*.google.lt/search*",
        "https://*.google.lu/search*",
        "https://*.google.nl/search*",
        "https://*.google.pl/search*",
        "https://*.google.pt/search*",
        "https://*.google.ro/search*",
        "https://*.google.sk/search*",
        "https://*.google.si/search*",
        "https://*.google.es/search*",
        "https://*.google.se/search*",
        "https://*.google.com/search*"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "expand_content.svg"
      ],
      "matches": [
        "https://*.google.at/*",
        "https://*.google.be/*",
        "https://*.google.bg/*",
        "https://*.google.hr/*",
        "https://*.google.cy/*",
        "https://*.google.cz/*",
        "https://*.google.dk/*",
        "https://*.google.ee/*",
        "https://*.google.fi/*",
        "https://*.google.fr/*",
        "https://*.google.de/*",
        "https://*.google.gr/*",
        "https://*.google.hu/*",
        "https://*.google.ie/*",
        "https://*.google.it/*",
        "https://*.google.lv/*",
        "https://*.google.lt/*",
        "https://*.google.lu/*",
        "https://*.google.nl/*",
        "https://*.google.pl/*",
        "https://*.google.pt/*",
        "https://*.google.ro/*",
        "https://*.google.sk/*",
        "https://*.google.si/*",
        "https://*.google.es/*",
        "https://*.google.se/*",
        "https://*.google.com/*"
      ]
    }
  ]
}
